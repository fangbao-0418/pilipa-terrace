.page-mission
  display: none

.no-access
  width: 100%;
  height: 100%
  background: #fff
  overflow: hidden
  position: relative
  .no-access-bgimg
    margin: 0 auto
    width: 280px
    height: 250px
    text-align: center
    top: 50%
    transform: translateY(-70%)
    position: relative
    background: url('../assets/images/no-access.png') center 0/280px 210px no-repeat
    .no-access-message
      color: #666
      position: absolute
      bottom: 0
      left: 0
      right: 0